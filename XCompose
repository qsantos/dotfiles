# vim: syntax=xcompose
<Multi_key> <p> <p> : "<"
<Multi_key> <p> <g> : ">"
<Multi_key> <p> <e> : "≤"
<Multi_key> <g> <e> : "≥"
<Multi_key> <equal> <asciitilde> : "≈"
<Multi_key> <asciitilde> <equal> : "≈"

<Multi_key> <x> <x> : "d(^.^)b"
<Multi_key> <y> <y> : "¯\\_(ツ)_/¯"

<Multi_key> <less> <less> : "≪"
<Multi_key> <greater> <greater> : "≫"

<Multi_key> <o> <k> : "✅"
<Multi_key> <k> <o> : "❌"
<Multi_key> <o> <N> : "☐"
<Multi_key> <o> <y> : "🗹"

<Multi_key> <O> <k> : "✓"
<Multi_key> <O> <K> : "✔"
<Multi_key> <O> <n> : "✗"
<Multi_key> <O> <N> : "✘"

<Multi_key> <i> <n> : "∞"
<Multi_key> <O> <O> : "Ɔ"
<Multi_key> <O> <o> : "ɔ"

<Multi_key> <n> <o> : "№"

<Multi_key> <T> <E> : "∃"
<Multi_key> <F> <A> : "∀"
<Multi_key> <S> <S> : "∑"
<Multi_key> <P> <P> : "∏"
<Multi_key> <S> <R> : "√"
<Multi_key> <E> <S> : "∅"
<Multi_key> <E> <O> : "∈"
<Multi_key> <N> <E> : "∉"
<Multi_key> <l> <l> : "⟦"
<Multi_key> <r> <r> : "⟧"

# https://news.ycombinator.com/item?id=962820
<Multi_key> <less> <3> : "♥"

# inspired from https://newton.cx/~peter/2013/04/typing-greek-letters-easily-on-linux/
# PKGW customizations
# Greek ambiguities: epsilon/eta, theta/tau, pi/phi/psi, omega/omicron

<Multi_key> <backslash> <comma> : " " U202F # thin nonbreaking space

